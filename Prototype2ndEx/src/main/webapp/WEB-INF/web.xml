<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
    version="4.0">

    <servlet>
        <servlet-name>CloneShapesServlet</servlet-name>
        <servlet-class>com.prototype.ex2.servlet.CloneShapesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CloneShapesServlet</servlet-name>
        <url-pattern>/CloneShapesServlet</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

</web-app>